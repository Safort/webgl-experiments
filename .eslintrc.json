{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "installedESLint": true,
  "plugins": [
    "react"
  ],
  "rules": {
    "treatUndefinedAsUnspecified": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-else-return": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "react/prop-types": 0
  }
}